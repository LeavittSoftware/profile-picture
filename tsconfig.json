{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es2016",
      "dom"
    ],
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "declaration": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "allowJs": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "rootDir": "src",
    "declarationDir": "types"
  },
  "include": [
    "bower_components/polymer/types/polymer-element.d.ts",
    "bower_components/polymer/types/lib/mixins/gesture-event-listeners.d.ts",
    "bower_components/polymer-decorators/polymer-decorators.d.ts",
    "bower_components/polymer-decorators/mixins/declarative-event-listeners.d.ts",
    "bower_components/lss-user-manager/types/lss-user-manager.d.ts",
    "bower_components/paper-dialog/paper-dialog.d.ts",
    "bower_components/paper-dialog-behavior/paper-dialog-behavior.d.ts",
    "lazy-import.d.ts",
    "src/**/*.ts"
  ]
}